projection;
strict ( 2 );
use draft;


define behavior for ZC_ANA_TRAVEL alias travel
//use etag
{
  use create;
  use update;
  use delete;
  use action accepTravel;
  use action rejectTravel;
  use association _Booking { create; with draft;}
  use action Edit;
  use action Activate;
  use action Discard;
  use action Prepare;
  use action Resume;
}

define behavior for ZC_ANA_BOOKING alias booking
//use etag
{
  use update;
  use delete;

  use association _Travel { with draft; }
}